@charset "utf-8";
/* CSS Document */

*{
	margin:0 auto;
	padding:0;
}


#main {
	width:100%;
	height:1000px;
	float:left;
}
#main .course-infos {
	background-color: #172a34;
	width:100%;
	height:240px;
	padding-top:0px;
	float:left;
}

#main .course-infos .w{
	width:1200px;
	height:240px;
	margin-left:90px;
	background:#172a34;
	float:left;
}
#main .course-infos .w .path{
	width:1200px;
	height:48px;
	margin:auto;	
	opacity: 1;
}
#main .course-infos .w .path p{
	width:450px;
	height:20px;
	padding-left:20px;
	padding-top:20px;
		
}
#main .course-infos .w .path p a{
 	text-decoration:none;
	font-size:12px;
	color:#a3aaae;
	
}
.path .path-split {
	margin: 0 6px;
	font-style: normal;
	color:#a3aaae;
}
.l{
	color:#FFF;
	font-size:28px;
	text-align:left;
	padding-left:20px;	
}

#main .course-infos .w .static{
	width:650px;
	height:70px;
	float:left;
	padding-top:25px;
}
#main .course-infos .w .static .static-item {
	display: inline-block;
	position: relative;
	width: 178px;
	height: 68px;
	padding-left: 21px;
	font-size: 18px;
}
.static-item2{
	display: inline-block;
	position: relative;
	padding-left: 21px;
	font-size: 18px;
	float: right;
}
.course-infos .static{
	color: #fff;
	font-size: 0;
	white-space: nowrap;
}
#main .course-infos .w .static .static-item .meta-value {
	display: block;
	margin: 5px 0 8px 0;
	opacity: .6;
	filter: alpha(opacity=60);
}
#main .course-infos .w .course-infos .static .static-item em {
	content: '';
	display: block;
	position: absolute;
	left: 0;
	top: 0;
	height: 100%;
	width: 1px;
	background-color: #fff;
	opacity: 0.1;
	filter: alpha(opacity=10);
	-ms-filter: alpha(opacity=10);
	zoom: 1;
}
#main .course-info-main{
	margin-top: 40px;
	float:left;	
	width:1041px;
	margin-left:74.5px;
}

#main .course-info-main .info-bar{
	width:240px;
	height:60px;
	float:right;
	background:#F00;
}
#main .course-info-main .info-bar p{
	width:150px;
	height:60px;	
	color:#639;
	padding-left:80px;
	padding-top:15px;
}
#main .course-info-main .info-bar p a{
		text-decoration:none;
		font-size:20px;	
		text-align:none;
		width:150px;
		
		height:20px;
		color:#FFF;
}
#main .course-info-main .info-bar p a:hover{
	color:black;	
}


