@charset "utf-8";
/* CSS Document */

/*已学的内容*/
.myspace-courselist{
	width:880px;
	float:right;
	margin:0;
	padding:0;
}

.my-space-course{
	display:inline-block;
	width:815px;
	margin-left:66px;
	margin-top:30px;
	border-left:1px solid #ccc;
}

.time{
	color:#8a8c8f;
	position:absolute;
	margin-left:-66px;
	margin-top:-5px;
}
#year{
	font-size:18px;
}
#md{
	font-size:12px;
}
.time::before {
	content:'';
	display:block;
	position:absolute;
	top:4px;
	left:61px;
	width:8px;
	height:8px;
	border-radius:50%;
	background-color:#d0d6d9;
}

.tl-item{
	width:815px;
	float:right;
}

.course-list-m ul{
	display:inline-block;
	padding-left:20px;
	width:850px;
	list-style:none;
}

.course-one{
	display:inline-block;
	width:260px;
	height:270px;
	float:left;
	margin-right:-7px;
	padding-left:15px;
}
.course-one:hover{
	transition:box-shadow 0.3s;
	box-shadow:-1px 1px 5px 1px #888888;
}
.course-one a{
	text-decoration:none;
}

.course-one img{
	padding-top:10px;
}

.course-list-img{
	width:100%;
	float:left;
}

.progress-val{
	width:240px;;
	background:#000;
	opacity:0.4;
	height:30px;
	position:absolute;
	margin-top:-37px;
	line-height:30px;
}
.progress-val span{
	font-size:20px;
	padding-left:10px;
	color:white;
}

.mask{
	height:2px;
	width:240px;
	background:green;
	position:absolute;
	margin-top:-7px;
}

#title-name{
	margin-top:10px;
	display:inline-block;
	color:black;
}
.text-ellipsis{
	overflow:hidden;
	display:inline-block;
	width:100%;
	text-overflow:ellipsis;
	white-space:nowrap;
	font-size:12px;
	color:#b4bbbf;
}
.dateandnum{
	color:#787d82;
	font-size:12px;
	padding-right:10px;
}

.time-label{
	display:none;
	height:20px;
	background:black;
	color:white;
	font-size:12px;
	padding:0 4px 0 4px;
	position:absolute;
	margin-top:-200px;
	opacity:0.5;
}
@-moz-keyframes move{
	0%{
		-moz-transform:translateX(40px);
		opacity:0;
	}
	100%{
		-moz-transform:translateX(0px);
		opacity:0.5;
	}
}
@-webkit-keyframes move{
	0%{
		-webkit-transform:translateX(40px);
		opacity:0;
	}
	100%{
		-webkit-transform:translateX(0px);
		opacity:0.5;
	}
}

/*删除*/
.del img{
	width:15px;
	height:15px;
	position:absolute;
	margin-top:-223px;
	margin-left:-7.5px;
}